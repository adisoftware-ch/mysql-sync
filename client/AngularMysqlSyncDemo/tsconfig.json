{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "mysql-sync-client": [
        "dist/mysql-sync-client"
      ],
      "mysql-sync-client/*": [
        "dist/mysql-sync-client/*"
      ],
      "mysql-sync-common": [
        "dist/mysql-sync-common"
      ],
      "mysql-sync-common/*": [
        "dist/mysql-sync-common/*"
      ]
    }
  }
}